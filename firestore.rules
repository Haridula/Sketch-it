rules_version='2'

service cloud.firestore {
  match /databases/{database}/documents {
    match /sketches/{document=**} {
      allow read, write: if true;
    }
    match /users/{userId}/storageStats/{document=**} {
      allow read, write: if true;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
